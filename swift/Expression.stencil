{% if expressionType == "identifier" %}{{ value }}
{% elif expressionType == "functionCall" %}{{ name }}({{ expressions|join:", " }})
{% elif expressionType == "operation" %}{{leftExpression}} {{operator}} {{rightExpression}}
{% elif expressionType == "integerLiteral" %}{{ value }}
{% elif expressionType == "booleanLiteral" %}{{ value }}
{% elif expressionType == "parentheses" %}({{ expression }})
{% elif expressionType == "listLiteral" %}[ {{ expressions|join:", " }} ]
{% elif expressionType == "stringLiteral" %}"{{ value }}"
{% elif expressionType == "templatedString" %}{% for expression in expressions %}"\({{expression}})"{% if not forloop.last %} + {% endif %}{% endfor %}
{% endif %}